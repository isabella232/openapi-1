{
  "name": "@zeit/openapi-website",
  "version": "0.0.1",
  "description": "UI for the OpenApi Spec",
  "scripts": {
    "now-build": "yarn cp-assets && webpack",
    "deploy": "yarn clean && yarn now-build && now --scope production",
    "cp-assets": "cpx 'src/{index.html,favicon.ico}' build/",
    "clean": "rimraf build"
  },
  "keywords": [
    "openapi",
    "zeit"
  ],
  "author": "Javi Velasco <javi@zeit.co>",
  "license": "MIT",
  "dependencies": {
    "@types/node": "12.0.10",
    "@types/react": "16.8.22",
    "@zeit/ncc": "0.20.1",
    "@zeit/open-api": "0.0.1",
    "cpx": "1.5.0",
    "css-loader": "3.0.0",
    "json-loader": "0.5.7",
    "postcss": "7.0.17",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-url": "8.0.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "rimraf": "2.6.3",
    "style-loader": "0.23.1",
    "swagger-ui": "3.22.3",
    "ts-loader": "6.0.4",
    "typescript": "3.5.2",
    "webpack": "4.35.0",
    "webpack-cli": "3.3.5",
    "yaml-loader": "0.5.0"
  }
}
